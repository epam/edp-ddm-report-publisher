    "query_hash": "6df840eb238a73a2a07cc9608fd50a49",
    "is_archived": false,
    "query": "SELECT\n    timestamp as \"Дата та час операції\",\n    request_id as \"Ідентифікатор запита\",\n    name as \"Ідентифікатор події в БД\",\n    user_keycloak_id as \"Ідентифікатор користувач\",\n    user_name as \"Користувач\",\n    source_system as \"Назва системи\",\n    source_application as \"Назва додатку\",\n    source_business_process as \"Назва бізнес процесу\",\n    source_business_process_definition_id as \"Ідентифікатор типу БП\",\n    source_business_process_instance_id as \"Ідентифікатор активного БП\",\n    source_business_activity as \"Назва кроку в БП\",\n    source_business_activity_id as \"Ідентифікатор кроку в БП\"\nFROM audit_event_security_event_v\nWHERE timestamp BETWEEN '{{ timestamp.start }}' AND '{{ timestamp.end }}'\n    AND application_name = 'keycloak'\n    AND action = 'SYSTEM_EVENT'\n    AND request_id ~* replace(regexp_replace('{{ request_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND name ~* replace(regexp_replace('{{ name }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND ( '( Всі значення )' IN ({{source_system}}) OR source_system IN ({{source_system}}) )\n    AND coalesce(source_business_process, 'null') ~* replace(regexp_replace('{{ source_business_process }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND coalesce(source_business_process_definition_id, 'null') ~* replace(regexp_replace('{{ source_business_process_definition_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND coalesce(source_business_process_instance_id, 'null') ~* replace(regexp_replace('{{ source_business_process_instance_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND coalesce(source_business_activity, 'null') ~* replace(regexp_replace('{{ source_business_activity }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND coalesce(source_business_activity_id, 'null') ~* replace(regexp_replace('{{ source_business_activity_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\nORDER BY timestamp DESC",
