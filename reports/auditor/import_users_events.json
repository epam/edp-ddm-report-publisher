          "query_hash": "8f5fd0ce748e440abb747c3b45b79d95",
          "is_archived": false,
          "query": "SELECT\n    timestamp as \"Дата та час операції\",\n    request_id as \"Ідентифікатор запиту\",\n    name as \"Назва події в БД\",\n    user_keycloak_id as \"Ідентифікатор адміністратора\",\n    user_name as \"ПІБ адміністратора\",\n    user_drfo as \"ДРФО адміністратора\",\n    source_application as \"Назва додатку/поди\",\n    user_id as \"ID створеного користувача\",\n    username as \"Username створеного користувача\",\n    enabled as \"Користувач активний\",\n    realm_id as \"Ідентифікатор реалму\",\n    realm_name as \"Ім'я реалму\",\n    client_id as \"Ім'я клієнта в keycloak\",\n    keycloak_client_id as \"Ідентифікатор клієнта в keycloak\",\n    roles as \"Ролі створеного користувача\",\n    katottg as \"KATOTTG створеного користувача\",\n    source_file_id as \"Ідентифікатор CSV файлу\",\n    source_file_name as \"Оригінальне ім'я CSV файлу\",\n    source_file_sha256_checksum as \"Контрольна сума CSV файлу (SHA256)\"\nFROM audit_event_system_event_v\nWHERE timestamp BETWEEN '{{ timestamp.start }}' AND '{{ timestamp.end }}'\n    AND request_id ~* replace(regexp_replace('{{ request_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND user_name ~* replace(regexp_replace('{{ user_name }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND user_drfo ~* replace(regexp_replace('{{ user_drfo }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND username ~* replace(regexp_replace('{{ username }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND source_file_id ~* replace(regexp_replace('{{ source_file_id }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\n    AND source_file_name ~* replace(regexp_replace('{{ source_file_name }}', '([()+.?[\\\\\\]])', '\\\\\\1', 'g'), '*', '.*')\nORDER BY timestamp DESC",
          "api_key": "mli6syxadcoTalyMIRryl3qtnFobdFtjA4WaMLp3",
